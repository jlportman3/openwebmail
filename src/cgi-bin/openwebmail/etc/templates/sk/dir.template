   function CheckAll()
   {
      for (var i=0;i<document.dirform.elements.length;i++)
      {
         var e = document.dirform.elements[i];
         if (e.type == 'checkbox' && 
             e.name != 'showthumbnail' &&
             e.name != 'showhidden' &&
             e.name != 'singlepage' &&
             e.name != 'allbox')
            e.checked = document.dirform.allbox.checked;
      }
   }
   function anyfileselected()
   {
      for (var i=0;i<document.dirform.elements.length;i++)
      {
         var e = document.dirform.elements[i];
         if (e.type == 'checkbox' && 
             e.name != 'showthumbnail' &&
             e.name != 'showhidden' && 
             e.name != 'singlepage' &&
             e.name != 'allbox' && 
             e.checked == 1 ) {
            return true;
         }
      }
      alert("Nebol vybraný ¾iadny adresár alebo súbor.");
      return false;
   }
   function selectedstr()
   {
      var str = '';
      for (var i=0;i<document.dirform.elements.length;i++)
      {
         var e = document.dirform.elements[i];
         if (e.type == 'checkbox' && 
             e.name != 'showthumbnail' &&
             e.name != 'showhidden' && 
             e.name != 'singlepage' &&
             e.name != 'allbox' && 
             e.checked == 1 ) {
            if (str == '') {
               str=e.value;
            } else {
               str=str+', '+e.value;
            }
         }
      }
      return(str);
   }
   function destnamefilled(target) {
      if (document.dirform.destname.value == '') {
         alert('Najskôr prosím zadajte < '+target+' >.');
         return false;
      } else {
         return true;
      }
   }
   function uploadfilled() {
      if (document.dirform.upload.value == '') {
         alert("Prosím najskôr si zvolte súbor, ktorý chcete nahra».");
         return false;
      } else {
         return true;
      }
   }
   function opconfirm(op, conf) {
      if (conf) {
         return(confirm('Ste si istý, ¾e chcete '+op+' vybrané polo¾ky? ( '+selectedstr()+' )'));
      } else {
         return true;
      }
   }
