   function submit_changed_dateselform() {
      document.dateselform.submit();
   }
//-->
</script>

<script src="<tmpl_var url_html escape="none">/javascript/validate.js" type="text/javascript"></script> 
<script src="<tmpl_var url_html escape="none">/javascript/calpopup.js" type="text/javascript"></script> 

<script type="text/javascript">//<![CDATA[
// calpopup.js tmpl vars
var wStart = <tmpl_var weekstart>;
var min_year = <tmpl_var min_year>;
// calpopup.js language and preferences
var wDay = new Array('Sunday','Monday','Tuesday','Wednesday','Thursday','Friday','Saturday');
var wDayAbbrev = new Array('S','M','T','W','T','F','S');
var wMonth = new Array('January','February','March','April','May','June','July','August','September','October','November','December');
var wOrder = new Array('1st','2nd','3rd','4th','last');
// validate.js language and preferences
var thisdayonlytxt = "This day's date only";
var everyoddmonththisyeartxt = "Every odd month this year";
var everyevenmonththisyeartxt = "Every even month this year";
function thewdayofthismonthtxt(wOrdn,wDayn) { return "The "+wOrder[(wOrdn-1)]+" "+wDay[(wDayn)]+" of the month"; }
function everywdaythismonthtxt(wDayn) { return "Every "+wDay[(wDayn)]+" this month"; }
function alerttxt(selectedmonth,daysinmonth,formYear,daysinmonth) { return "Sorry, "+selectedmonth+" only has "+daysinmonth+" days in "+formYear+".\n"+"Changing the day to "+daysinmonth+"."; }
//]]></script> 

