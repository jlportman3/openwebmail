              <option value="TZ.VALUE">Timezone</option>
              <option value="GEO.VALUE.LONGITUDE">Longitude</option>
              <option value="GEO.VALUE.LATITUDE">Latitude</option>
              <option value="MAILER.VALUE">Mailer Program</option>
              <option value="NOTE.VALUE">Note</option>
              <option value="CLASS.VALUE">Public or Private</option>
              <option value="SOURCE.VALUE">Contact Source URL</option>
              <option value="NAME.VALUE">Contact Source URL Description</option>
              <option value="UID.VALUE">External Unique ID</option>
              <option value="PRODID.VALUE">Contact Creator Program</option>
