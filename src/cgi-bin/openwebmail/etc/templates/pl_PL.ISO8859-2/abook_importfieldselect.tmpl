          <td class="<tmpl_if odd>rowdark<tmpl_else>rowlight</tmpl_if>"><tmpl_var datasampletwo escape="html"></td>
          <td class="<tmpl_if odd>rowdark<tmpl_else>rowlight</tmpl_if>"><tmpl_var datasamplethree escape="html"></td>
