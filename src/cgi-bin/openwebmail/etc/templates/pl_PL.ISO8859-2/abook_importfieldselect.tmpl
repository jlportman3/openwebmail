              <option value="ADR.VALUE.EXTENDEDADDRESS">Address: Suite / Apartment</option>
              <option value="ADR.VALUE.POSTOFFICEADDRESS">Address: P.O. Box</option>
              <option value="ADR.VALUE.LOCALITY">Address: City</option>
              <option value="ADR.VALUE.REGION">Address: State / Province</option>
              <option value="ADR.VALUE.POSTALCODE">Address: Zip / Postal Code</option>
              <option value="ADR.VALUE.COUNTRY">Address: Country</option>
